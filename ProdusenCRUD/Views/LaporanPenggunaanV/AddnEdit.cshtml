@model ProdusenCRUD.Models.laporankedatanganvaksin
@{
    ViewBag.Title = "AddnEdit";
}

@using (Html.BeginForm("AddnEdit", "LaporanPenggunaanV", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.IdLaporan)
    <div class="form-group">
        @Html.LabelFor(model => model.NoRegisVaksin, new { @class = "control-label" })
        @Html.EditorFor(model => model.NoRegisVaksin, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.NoRegisVaksin)
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.IdProdusen, new { @class = "control-label" })
        @Html.EditorFor(model => model.IdProdusen, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.IdProdusen)

    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.IdBPOM, new { @class = "control-label" })
        @Html.EditorFor(model => model.IdBPOM, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.IdBPOM)
    </div>


    <div class="form-group">
        @Html.LabelFor(model => model.StatusVaksin, new { @class = "control-label" })
        @Html.EditorFor(model => model.StatusVaksin, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.StatusVaksin)
    </div>


    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
        <input type="reset" value="Reset" class="btn" />
    </div>
}

