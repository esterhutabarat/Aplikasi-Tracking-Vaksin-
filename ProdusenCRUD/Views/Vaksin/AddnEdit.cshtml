@model ProdusenCRUD.Models.Vaksin
@{
    ViewBag.Title = "AddnEdit";
}

@using (Html.BeginForm("AddnEdit", "Vaksin", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.IdVaksin)
    <div class="form-group">
        @Html.LabelFor(model => model.NoRegisVaksin, new { @class = "control-label" })
        @Html.EditorFor(model => model.NoRegisVaksin, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.NoRegisVaksin)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.Status, new { @class = "control-label" })
        @Html.EditorFor(model => model.Status, new { htmlAttributes = new   { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.Status)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.IdProdusen, new { @class = "control-label" })
        @Html.EditorFor(model => model.IdProdusen, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.IdProdusen)

    </div>

    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
        <input type="reset" value="Reset" class="btn" />
    </div>
}

